<h2>Recipes</h2>
<div class="recipe-grid">
    <% recipes.forEach(function(recipe){ %>
        <div class="recipe-card">
            <img src="<%= recipe.image %>" alt="<%= recipe.title %>" class="recipe-image">
            <!-- <h3><%= recipe.title %></h3>
            <h3><%= recipe.image  %></h3> -->
        </div>
    <% }); %>
</div>
<br>